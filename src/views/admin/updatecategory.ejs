<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
   <form action="/admin/update-category/<%= categoryid %>" method="post">
    <input type="text" name="categoryName" value="<%= categoryName %>">
    <input type="submit">
   </form>
    
    <% if (error) { %>
        <div class="error-message">
            <%= error %>
        </div>
    <% } %>

    <% if (message) { %>
        <div class="success-message">
            <%= message %>
        </div>
    <% } %>
    <a href="/admin/add-category">Go Back</a>
</body>
</html>